package com.java.sort_match;

public class Sort {

	public static void main(String[] args) {
		int a[] = { 1, 2, 2, 3, 4, 5, 6, 1, 2, 1 };

		for (int i = 0; i < a.length; i++) {
			int temp = 0;
			for (int j = i + 1; j < a.length; j++) {
				if (a[i] == a[j]) {
					temp = a[j];
					a[j] = a[i + 1];
					a[i + 1] = temp;
				}
			}
			System.out.print(a[i] + " ");
		}
	}
}
