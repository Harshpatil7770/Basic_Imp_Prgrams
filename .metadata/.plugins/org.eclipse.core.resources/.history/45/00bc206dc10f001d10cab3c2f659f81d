package com.java.sort_match;

public class Sort {

	public static void main(String[] args) {
		int a[] = { 1, 2, 1, 3, 4, 5, 6,3,2, 1, 2, 1 };

		for (int i = 0; i < a.length; i++) {
			int temp = 0;
			int n=0;
			for (int j = i + 1; j < a.length-n; j++) {
				if (a[i] == a[j]) {
					temp = a[j];
					a[j] = a[i + 1+n];
					a[i + 1+n] = temp;
					n++;
				}
			}
			if(a[i]==a[i+1]) {
				i++;
			}
		}
	}
}
